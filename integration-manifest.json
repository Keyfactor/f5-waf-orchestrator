        "supportsDiscovery": true,
        "supportsManagementAdd": true,
        "supportsManagementRemove": true,
        "supportsReenrollment": false,
        "supportsInventory": true,
        "platformSupport": "Unused"
      },
      "linux": {
        "supportsCreateStore": false,
        "supportsDiscovery": true,
        "supportsManagementAdd": true,
        "supportsManagementRemove": true,
        "supportsReenrollment": false,
        "supportsInventory": true,
        "platformSupport": "Unused"
      },
      "store_types": {
        "f5WafTls": {
          "Name": "F5 WAF TLS",
          "ShortName": "f5WafTls",
          "Capability": "f5WafTls",
          "SupportedOperations": {
            "Add": true,
            "Create": false,
            "Discovery": true,
            "Enrollment": false,
            "Remove": true
          },
          "Properties": [],
